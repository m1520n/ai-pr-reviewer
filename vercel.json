{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "api/webhook.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.js",
      "maxDuration": 60
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
} 