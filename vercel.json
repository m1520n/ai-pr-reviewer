{
  "version": 2,
  "functions": {
    "api/**/*.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/webhook.js"
    },
    {
      "src": "/callback",
      "dest": "/api/callback.js"
    },
    {
      "src": "/",
      "dest": "/api/index.js"
    }
  ]
} 